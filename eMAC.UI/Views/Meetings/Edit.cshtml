<div class="row" >
    <div class="col-md-12 RbtnMargin">
        <button kendo-button class="pull-right" ng-click="cancel()"><span class="k-icon k-i-cancel"></span>Cancel</button>
        <button kendo-button class="pull-right" ng-click="save()"><span class="k-icon k-i-tick"></span>Save</button>
        <button kendo-button class="pull-right"><span class="k-icon k-i-tick"></span>Submit</button>
    </div>
