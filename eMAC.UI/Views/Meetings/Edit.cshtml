<div class="panel panel-default" data-ng-controller="mtgsEditController">
    <div class="panel-heading">Meeting details</div>
  <div class="panel-body">
    <div class="form-horizontal">
        <div class="row clearfix">           
          <div class="col-lg-12">
            <div class="form-group clearfix col-lg-10">                            
                <label class="col-lg-1 control-label" for="mtgNumber" style="padding-left: 0px;">Meeting no.</label>                                                
                <div class="col-lg-5">
                    <input disabled ng-model="meeting.mtg_number" type="text" class="form-control">
                </div>
            </div>  
            <div class="col-lg-2 form-group clearfix">
                <label class="col-lg-4 control-label" for="mtgStatus">Status</label>
                <div class="col-lg-8">
                    <input ng-model="meeting.status" type="text" class="form-control">
                </div>
             </div>
            <div class="col-lg-10 form-group clearfix">
                @*Title*@
         
                    <label class="col-lg-1 control-label" for="mtgTitle">Title</label> 
                    <div class="col-lg-11">
                    <input ng-model="meeting.mtg_title" type="text" class="form-control">
                    </div>                      
                             
            </div>
            <div class="col-lg-10 form-group clearfix">
                 <div class="col-lg-6" style="padding: 0px;">
                    <label class="col-lg-2 control-label" for="mtgDurationFromDate">Duration</label>
                    <div class="col-lg-4">
                        <input kendo-date-picker id="dateDurFr" k-format="'dd MMM yyyy'" k-options="datePickerOptions" k-ng-model="meeting.start_date" />
                        @*<input ng-model="job.number" type="date" class="form-control">*@                                
                    </div>
                    <label class="col-lg-1 control-label" for="jobNumber">to</label> 
                    <div class="col-lg-4">
                        <input kendo-date-picker id="dateDurTo" k-format="'dd MMM yyyy'" k-options="datePickerOptions" k-ng-model="meeting.end_date" />
                            @*<input ng-model="job.number" type="date" class="form-control">*@
                    </div>     
               </div>
                <div class="col-lg-6" style="padding: 0px;">
                <label class="col-lg-2 control-label" for="jobNumber">Venue</label>
                <div class="col-lg-6">
                    <input ng-model="meeting.venue" type="text" class="form-control">
                </div>
                <div class="col-xs-4 col-md-4">
                    <select ng-model="meeting.region" class="form-control" ng-options="s.abbr as s.name for s in states">
                        <option value="">--</option>
                    </select>
                </div>
                    </div>
            </div>
         </div>           
      </div> 
