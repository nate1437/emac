                                <select ng-model="meeting.ctry_code" class="form-control" ng-options="s.ctry_code as s.ctry_code for s in states">
                                    <option value="">--</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-lg-2">
                            <label class="col-lg-3 control-label" for="mtgStatus">Status</label>
                            <div class="col-lg-9" style="padding-right: 0px;">
                                <input ng-model="meeting.status" type="text" class="form-control">
