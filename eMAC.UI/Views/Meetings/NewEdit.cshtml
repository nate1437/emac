@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@*<div style="padding:10px;background-color: lightsteelblue;padding: 10px;border: thin solid darkgray;border-radius: 4px;">
    <h4>Meeting elements</h4>
</div>*@

<link href="~/Content/mtg-edit.css" rel="stylesheet" />
<div ng-cloak>
    <div class="row" style="margin-top: 0;" ng-cloak>
        <div class="col-md-12 RbtnMargin">
            <div class="pull-left">
                <button type="button" class="btn btn-default btn-xs"
                    ng-click="print()">
                    <span class="glyphicon glyphicon-print" aria-hidden="true"></span> Print preview
                </button>
                <button type="button" class="btn btn-success btn-xs"
                    ng-show="(approveDetailBtnEnable && meeting.status=='Submitted for SPMC') || (approveDetailBtnEnable && meeting.status=='Draft')"
                    ng-disabled="meeting.status=='Draft'"
                    ng-click="confirmApprove()">
                    <span class="glyphicon glyphicon-ok" aria-hidden="true"></span> Approve - SPMC
                </button>
                <button type="button" class="btn btn-warning btn-xs"
                    ng-show="(rejectDetailBtnEnable && meeting.status=='Submitted for SPMC') || (approveDetailBtnEnable && meeting.status=='Draft')"
                    ng-disabled="meeting.status=='Draft'"
                    ng-click="confirmReject()">
                    <span class="glyphicon glyphicon-remove" aria-hidden="true"></span> Return
                </button>
                <button type="button" class="btn btn-info btn-xs"
                    ng-show="(finalDetailBtnEnable && (meeting.status == 'Approved for SPMC' || meeting.status == 'Submitted for Finalization')) || (finalDetailBtnEnable && (meeting.status=='Revise for SPMC' || meeting.status=='Revise for Finalization'))"
                    ng-disabled="meeting.status=='Revise for SPMC' || meeting.status=='Revise for Finalization'"
                    ng-click="confirmFinalize()">
                    <span class="glyphicon glyphicon-ok" aria-hidden="true"></span> Finalize
                </button>
                <button type="button" class="btn btn-danger btn-xs"
                    ng-show="(reviseDetailBtnEnable && (meeting.status == 'Approved for SPMC' || meeting.status == 'Submitted for Finalization')) || (finalDetailBtnEnable && (meeting.status=='Revise for SPMC' || meeting.status=='Revise for Finalization'))"
                    ng-disabled="meeting.status=='Revise for SPMC' || meeting.status=='Revise for Finalization'"
                    ng-click="confirmReject()">
                    <span class="glyphicon glyphicon-remove" aria-hidden="true"></span> Revise
                </button>
                <button type="button" class="btn btn-default btn-xs"
                    ng-show="cancelDetailBtnEnable"
                    ng-disabled="meeting.status=='Finalized' || meeting.status=='Cancelled'"
                    ng-click="confirmCancel()">
                    <span class="glyphicon glyphicon-remove" aria-hidden="true"></span> Cancel meeting
                </button>
            </div>
            <div class="pull-right">
                <button type="button" class="btn btn-default btn-xs"
                    ng-click="save()"
                    ng-disabled="isUser && (isAccessible || meeting.status=='Submitted for SPMC' || meeting.status=='Submitted for Finalization' || meeting.status=='Approved for SPMC' || meeting.status=='Finalized' || meeting.status=='Cancelled')"
                    ng-show="saveDetailBtnEnable">
                    <span class="glyphicon glyphicon-ok-circle"></span> Save</button>
                <button type="button" class="btn btn-default btn-xs"
                    ng-click="confirmSubmit()"
                    ng-disabled="isUser 
                    && (
                        isAccessible  
                        || meeting.status=='Submitted for SPMC'
                        || meeting.status=='Submitted for Finalization'
                        || meeting.status=='Approved for SPMC'
                        || meeting.status=='Finalized'
                        || meeting.status=='Cancelled'
                    )"
                    ng-show="submitDetailBtnEnable">
                    <span class="glyphicon glyphicon-ok-circle"></span> Submit</button>
                <button type="button" class="btn btn-default btn-xs"
                    ng-if="!isApprover && (unAuthorized || isAccessible)"
                    ng-click="cancel()">
                    <span class="glyphicon glyphicon-remove-circle"></span> Close</button>
                <button type="button" class="btn btn-default btn-xs"
                    ng-if="isAdmin"
                    ng-click="events.confirmDelete.click()"><span class="glyphicon glyphicon-erase"></span> Delete
                </button>
                <button type="button" class="btn btn-default btn-xs"
                    ng-if="isApprover || (!unAuthorized && !isAccessible)"
                    ng-click="confirmClose()">
                    <span class="glyphicon glyphicon-remove-circle"></span> Close</button>
            </div>
        </div>
    </div>
    <div class="panel panel-default" ng-cloak>
        <div class="panel-heading">
            <span class="">Meeting details</span>
            <span class="pull-right">Last updated by {{meeting.updated_by}} on {{meeting.date_updated}} 
            </span>
        </div>
        <div class="panel-body" style="min-height: 650px;" ng-cloak>
            <div class="form-horizontal">
                <div class="form-group clearfix required">
                    <div class="row">
                        <div class="col-lg-12">
                            <div class="col-lg-3 col-md-3 col-sm-4 col-xs-12">
                                <label class="col-lg-5 col-md-6 col-sm-6 col-xs-12 control-label" for="mtgNumber">Meeting no.</label>
                                <div class="col-lg-7 col-md-6 col-sm-6 col-xs-12" style="padding: 0px;">
                                    <input readonly ng-model="meeting.mtg_no" type="text" class="form-control" >
                                </div>
                            </div>
                            <div class="col-lg-7 col-md-7 col-sm-5 col-xs-12">
                                <label class="col-lg-1 col-md-1 col-sm-2 col-xs-12 control-label" for="mtgTitle">Title</label>
                                <div class="col-lg-11 col-md-11 col-sm-10 col-xs-12" style="padding: 0px;">
                                    <input ng-model="meeting.mtg_title" type="text" id="mtgTitle" class="form-control">
                                </div>
                            </div>
                            <div class="col-lg-2 col-md-2 col-sm-3 col-xs-12">
                                <label class="col-lg-5 col-md-6 col-sm-7 col-xs-12 control-label" for="mtgStatus">Status</label>
                                <div class="col-lg-7 col-md-6 col-sm-5 col-xs-12" style="padding: 0px;">
                                    <input ng-model="meeting.status" type="text" class="form-control" disabled>
                                </div>
                            </div>
                        </div>
                    </div>
                        
                </div>
                <div class="form-group clearfix required">
                    <div class="row">
                        <div class="col-lg-5 col-md-6 col-sm-5 col-xs-12">
                            <div class="col-lg-12">
                                <label class="col-lg-3 col-md-3 control-label" for="mtgDurationFromDate">Duration</label>
                                <div class="col-lg-9 col-md-9" style="padding: 0px;">
                                    <input class="myPicker" ng-model="meeting.start_date" style="width: 157px;" /> 
                                    <label class="control-label control-label-z" for="mtgDurationToDate">to</label>
                                    <input id="mtg-end-date" class="myPicker" ng-model="meeting.end_date" style="width: 157px;" />
                                </div>
                                <input ng-model="meeting.end_date" type="text" class="form-control" name="title" required style="display: none">
                                <input ng-model="meeting.start_date" type="text" class="form-control" name="title" required style="display: none">
                            </div>
                            <div class="col-lg-12" style="margin-top:5px;">
                                <label class="col-lg-3 control-label control-label-z" for="jobNumber">Venue</label>
                                <div class="col-lg-3" style="padding: 0px;">
                                    <input ng-model="meeting.venue" type="text" class="form-control">
                                </div>
                                <div class="col-lg-6" style="padding: 0px; padding-left:5px !important">
                                    <select ng-model="meeting.ctry_code" class="form-control" ng-options="s.ctry_code as s.ctry_name for s in states">
                                        <option value="">--</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-7 col-md-6 col-sm-7 col-xs-12">
                            <div class="col-lg-6" style="padding: 0px;" >
                                <div class="col-lg-12"style="padding: 0px;" >
                                    <label class="col-lg-4 control-label" for="jobNumber" >Org. unit</label>
                                    <div class="col-lg-8" style="padding: 0px; ">
                                        <select ng-model="meeting.org_unit" class="form-control" ng-options="s.org_unit as s.org_unit for s in units">
                                            <option value="">--</option>
                                        </select>
                                    </div>
                                </div>
                                <div class="col-lg-12" style="padding: 0px;margin-top:5px;">
                                    <label class="col-lg-4 control-label" for="mtgNumber" >Resp. officer</label>
                                    <div class="col-lg-8" style="padding: 0px;" >
                                        <select ng-model="meeting.resp_officer" class="form-control" ng-options="o.user_name as o.resp_officer_name for o in officers">
                                            <option value="">--</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                            <div class="col-lg-6" style="padding: 0px;" >
                                <div class="col-lg-12" >
                                    <label class="col-lg-4 control-label control-label-z" for="mtgNumber">Co-resp. officer</label>
                                    <div class="col-lg-8" style="padding: 0px;" >
                                        <select ng-model="meeting.co_resp_officer" class="form-control" ng-options="o.user_name as o.resp_officer_name for o in officers">
                                            <option value="">--</option>
                                        </select>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>  
            </div>

            <div class="form-horizontal">
                <hr style="margin-top: 5px !important;"/>
                <div class="row clearfix">
                    <div class="col-lg-12">
                        <div class="col-lg-2" style="padding-right: 0;">
                            <div class="form-group clearfix">
                                <div class="col-lg-12 col-xs-12">
                                    <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline">
                                        <input id="spmc_waived" class="styled ng-pristine ng-untouched ng-valid" type="checkbox" ng-model="meeting.is_spmc_waived">
                                        <label for="spmc_waived">SPMC waived?</label>
                                    </div> 
                                </div>
                            </div>
                            <div class="form-group clearfix">
                                <div class="col-lg-12 col-xs-12" style="padding-right: 0;">
                                    <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline">
                                        <input id="is_four_mos_waiver_approved" class="styled ng-pristine ng-untouched ng-valid" type="checkbox" ng-model="meeting.is_four_mos_waiver_approved">
                                        <label for="is_four_mos_waiver_approved">4-month waiver approved?</label>
                                    </div> 
                                    
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-2" style="padding-right: 0;">
                            <div class="form-group clearfix">
                                <div class="col-lg-12 col-xs-12">
                                    <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline">
                                        <input id="is_not_wpro_mtg" class="styled ng-pristine ng-untouched ng-valid" type="checkbox" ng-model="meeting.is_not_wpro_mtg">
                                        <label for="is_not_wpro_mtg">Not a WPRO-led meeting?</label>
                                    </div> 
                                </div>
                            </div>
                            <div class="form-group clearfix">
                                <div class="col-lg-12 col-xs-12">
                                    <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline">
                                        <input id="incl_in_events_calendar" class="styled ng-pristine ng-untouched ng-valid" type="checkbox" ng-model="meeting.incl_in_events_calendar">
                                        <label for="incl_in_events_calendar">Include in Event calendar?</label>
                                    </div> 
                                   
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-4">
                            <div class="form-group clearfix">
                                <div class="col-lg-12">
                                    <label class="col-lg-5 control-label" for="jobNumber">Meeting type</label>
                                    <div class="col-xs-12 col-lg-7">
                                        <select ng-model="meeting.mtg_type" class="form-control ng-pristine ng-untouched ng-valid" ng-options="option.meeting_type_id as option.meeting_type_name for option in types"><option ng-disabled="true" ng-selected="true" value="" class="" selected="selected" disabled="disabled">Select type</option><option value="number:19" label="Meetings for the Exchange of Information">Meetings for the Exchange of Information</option><option value="number:20" label="Meeting of Experts" selected="selected">Meeting of Experts</option></select>
                                    </div>
                                </div>
                            </div>
                            <div class="form-group clearfix">
                                <div class="col-lg-12">
                                    <label class="col-lg-5 control-label" for="jobNumber">Meeting classification</label>
                                    <div class="col-xs-12 col-lg-7">
                                        <select ng-model="meeting.mtg_classification" class="form-control ng-pristine ng-untouched ng-valid" ng-options="option.meeting_classification_id as option.meeting_classification_name for option in classifications | filter:{meeting_type_id:meeting.mtg_type}"><option ng-disabled="true" ng-selected="true" value="" class="" selected="selected" disabled="disabled">Select classification</option><option value="number:7" label="Advisory committee">Advisory committee</option><option value="number:17" label="Working group">Working group</option><option value="number:15" label="Task force">Task force</option><option value="number:13" label="Meetings of subcommittees of advisory committees">Meetings of subcommittees of advisory committees</option><option value="number:10" label="Meeting">Meeting</option><option value="number:11" label="Other Scientific/Technical committee-type meeting">Other Scientific/Technical committee-type meeting</option></select>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                        <div class="col-lg-4">
                            <div class="form-group clearfix">
                                <div class="col-lg-12 col-xs-12" style="padding-top: 0px;">
                                    <label class="col-lg-7 control-label" for="jobNumber">SPMC approval date</label>
                                    <div class="col-lg-5">
                                        <input class="myPicker" ng-model="meeting.spmc_approval_date" style="width: 110px;" ng-disabled="isUser" />
                                    </div>
                                </div>
                            </div>
                            <div class="form-group clearfix">
                                <div class="col-lg-12 col-xs-12" style="padding-left: 0px;">
                                    <label class="col-lg-3 control-label" for="jobNumber"></label>
                                    <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline">
                                        <input id="needs_summary_report" type="checkbox" ng-disabled="isUser" ng-model="meeting.needs_summary_report">
                                        <label for="needs_summary_report">Summary Report?</label>
                                    </div> 
                                    <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline">
                                        <input id="needs_final_report" type="checkbox" ng-model="meeting.needs_final_report" ng-disabled="isUser || meeting.mtg_classification==6 || meeting.mtg_classification==8">
                                        <label for="needs_final_report">Final Report?</label>
                                    </div> 
                                </div>
                            </div>
                            
                        </div>
                    </div>
                </div>
            </div>
            
            <div class="form-horizontal">
                <hr style="margin-top: 5px !important;"/>
                <div class="tabset" ng-cloak>
                    <tabset class="tabset-inner">
                    <tab heading="Planning document" active="tabset.planning_doc">
                        <div class="" ng-cloak>
                            <div class="tabset">
                                <tabset class="tabs-left">
                                    <!--INTERNAL PLANNING-->       
                                    <tab heading="Internal planning">
                                        <div class="row clearfix">                            
                                            <div class="col-lg-9" id="internal-planning">
                                                <h5>Internal planning</h5>
                                                <hr />
                                                <div class="row clearfix">                                            
                                                    <div class="col-lg-12 form-group clearfix">      
                                                        <div class="col-lg-12" style="padding-top:0px;">
                                                            <label class="pull-left control-label" style="padding-right:10px;">Date</label>
                                                            <div class="pull-left" style="padding-right:20px;">
                                                                <input class="myPicker" ng-model="meetingDetail.planning_mtg_date" />                                                                
                                                            </div>
                                                            <label class="pull-left control-label" style="padding-right:10px;">Time</label>
                                                            <div class="pull-left" style="padding-right:20px;">                                                                
                                                                <div class="">
                                                                    <input kendo-time-picker maxlength="5" ng-keydown="meetingTimeValidate($event)" k-options="meetingTimeOptions" ng-model="meetingDetail.planning_mtg_time" style="width:100px;" />
                                                                </div>
                                                            </div>
                                                            <label class="pull-left control-label" style="padding-right:10px;">Room</label>
                                                            <div class="pull-left">
                                                                <input ng-model="meetingDetail.planning_mtg_room" type="text" class="form-control" maxlength="20">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <hr />
                                                <div class="row clearfix">                               
                                                    <div class="col-lg-6">
                                                        <div class="form-group">
                                                            <label class="col-md-12 edit-form-label" for="bg-info">Attendees/Present</label>
                                                            <div class="col-md-12">                                                           
                                                                <textarea id="ta-present"></textarea>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6">
                                                        <div class="form-group">
                                                        <label class="col-md-12 edit-form-label" for="bg-info">Invited-unable to attend</label>
                                                        <div class="col-md-12">
                                                        
                                                            <textarea id="ta-absent"></textarea>
                                                        </div>
                                                            </div>
                                                    </div>                                   
                                                </div> 
                                            </div>
                                        </div>
                                    </tab>
                                    <!--BACKGROUND-->       
                                    <tab heading="1. Background">
                                        <div class="col-lg-9" id="background">
                                            <div class="row">
                                                <div class="col-lg-12 col-md-12 col-sm-12">
                                                    <h5>1. Background information</h5>
                                                </div>
                                                <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12">
                                                    <hr style="margin-top: 0px !important;"/>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                        <div class="col-lg-12" >
                                                            <span>Background Information</span>                                                          
                                                        </div>
                                                        <div class="col-lg-12"> 
                                                            <textarea id="ta-bg"></textarea>
                                                            <span class="text-muted" style="font-style:italic">Please provide information directly related to the need and objectives of the meeting. Please limit to 300 words. Word count <span class="word-counter" >{{wordCountBackground}}</span></span>                                   
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="col-lg-6">
                                                    <div class="form-group">
                                                    
                                                        <div class="col-lg-12" >
                                                            <span>Summary</span>                                                          
                                                        </div>
                                                        <div class="col-lg-12">
                                                            <textarea id="ta-summ"></textarea>
                                                            <span class="text-muted" style="font-style:italic">Please provide two short paragraphs including the most important points for the invitation letter. Please limit to 300 words. Word count <span class="word-counter">{{wordCountSummary}}</span></span>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </tab>
                                    
                                    <!--CONTRIBUTION TO WHO-->      
                                    <tab heading="2. Contribution to WHO's work">
                                        <div class="col-lg-9">
                                            <div class="row" ng-controller="lnkgsController" >
                                                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                                                    <h5>2. Contribution to WHO's work</h5>
                                                </div>
                                                <div class="col-lg-12 col-md-12 col-xs-12 col-sm-12">
                                                    <hr style="margin-top: 0px !important;"/>
                                                </div>
                                                <div class="col-lg-12">
                                                    <div class="tabset-inner">
                                                        <tabset>
                                                            <tab heading="Core functions">
                                                                <div class="row" style="margin-top: 10px;">
                                                                    <div class="col-lg-12" style="min-height: 322px !important "> 
                                                                        <span>2a. Related WHO core functions</span>                                                            
                                                                        <hr />
                                                                        <div class="form-group clearfix" ng-repeat="cfx in mtgCoreFxObject">  
                                                                            <div class="col-lg-12 col-xs-12">
                                                                                <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline">
                                                                                    <input ng-attr-id="{{ cfx.mtg_core_function_id }}" class="styled" type="checkbox" ng-model="selMtgCoreFx[cfx.mtg_core_function_id]" ng-checked="cfx.is_selected">
                                                                                    <label ng-attr-for="{{ cfx.mtg_core_function_id }}">{{cfx.core_function_name}}</label>
                                                                                </div> 
                                                                            </div>                        
                                                                        </div>
                                                                    </div>
                                                                </div>                                         
                                                            </tab>                                 
                                                            <tab heading="Relevant PB deliverables" ng-click="pbDeliverablesTab()">
                                                                <div class="row" style="margin-top: 10px;">
                                                                    <div class="col-lg-12">
                                                                        <span>2b. Contribution to WHO's programme budget</span><br />
                                                                        <span class="text-muted" style="font-style:italic; margin-left: 22px;">Identify the specific output numbers and deliverables</span> 
                                                                        
                                                                        <div class="pull-right">
                                                                            <button type="button" class="btn btn-primary" style="margin-top: -20px;" ng-click="editPbdeliverables()"><span class="glyphicon glyphicon-edit"></span> Edit</button>
                                                                        </div>
                                                                    </div>
                                                                    <div class="col-lg-12">
                                                                        <hr />
                                                                    </div>
                                                                    <div class="col-lg-12">
                                                                        <span>The workshop would also contribute to:</span>
                                                                    </div>
                                                                    <div class="col-lg-12" style="overflow-y: auto; max-height: 250px !important; margin-bottom: 2px !important;">
                                                                        <div class="form-group clearfix" ng-repeat="pbCategory in pbCategories" ng-if="selectedPbCategory[pbCategory.id]">  
                                                                            <hr />
                                                                            <div class="col-lg-12 col-xs-12">
                                                                                <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline">
                                                                                    <input ng-model="selectedPbCategory[pbCategory.id]" ng-if="selectedPbCategory[pbCategory.id]" class="styled" type="checkbox" ng-model="selectedPbCategory[pbCategory.id]" disabled>
                                                                                    <label ng-if="selectedPbCategory[pbCategory.id]">{{pbCategory.desc}}</label>
                                                                                    <div class="form-group clearfix" ng-repeat="pbOutcome in pbOutcomes" ng-if="pbOutcome.pbcategoryid == pbCategory.id && selectedPbCategory[pbCategory.id] && selectedPbOutcome[pbOutcome.id]">  
                                                                                        <div class="col-lg-12 col-xs-12" style="padding-left:40px;" ng-if="selectedPbOutcome[pbOutcome.id]">
                                                                                            <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline" ng-if="selectedPbOutcome[pbOutcome.id]">
                                                                                                <input ng-model="selectedPbOutcome[pbOutcome.id]" class="styled" type="checkbox" ng-model="selectedPbOutcome[pbOutcome.id]" ng-if="selectedPbOutcome[pbOutcome.id]" disabled>
                                                                                                <label ng-if="selectedPbOutcome[pbOutcome.id]" ng-if="selectedPbOutcome[pbOutcome.id]">{{pbOutcome.desc}}</label>
                                                                                                <div class="form-group clearfix" ng-repeat="pbOutput in pbOutputs" ng-if="pbOutput.pboutcomeid == pbOutcome.id && selectedPbOutcome[pbOutcome.id] && selectedPbOutput[pbOutput.id]" >  
                                                                                                    <div class="col-lg-12 col-xs-12" style="padding-left:40px;" >
                                                                                                        <div class="checkbox abc-checkbox abc-checkbox-primary checkbox-inline" ng-if="selectedPbOutput[pbOutput.id]">
                                                                                                            <input ng-model="selectedPbOutput[pbOutput.id]" class="styled" type="checkbox" ng-model="selectedPbOutput[pbOutput.id]" ng-if="selectedPbOutput[pbOutput.id]" disabled>
                                                                                                            <label ng-if="selectedPbOutput[pbOutput.id]" ng-if="selectedPbOutput[pbOutput.id]">{{pbOutput.desc}}</label>
                                                                                                        </div>
                                                                                                    </div>
                                                                                                </div>
                                                                                            </div> 
                                                                                        </div>                        
                                                                                    </div>
                                                                                </div> 
                                                                            </div>                        
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                                
                                                                <div class="modal modal-vcenter fade" id="pb_select" tabindex="-1" role="dialog">
                                                                    <div class="modal-dialog modal-lg" role="document">
                                                                        <div class="modal-content">                                                                    
                                                                            <div class="modal-header bg-info"><span class="glyphicon glyphicon-info-sign"></span> Relevant PB deliverables </div>
                                                                            <div class="modal-body">
